#python3.6
#read transcript from _unf.gtf file and compare each transcript in annotation file
#output coordinates of each transcript as follows
#trans_id chr start end 
#ARGV={path_to_unf,path_to_annotaion,output}
if # of ARGV != 3
	print usageinfo
	die
{$unf,$ann,$out}={open(ARGV[0]),open(ARGV[1]),open(ARGV[2])} #if failed to open, die
$err_log=open(ARGV[2].err)#log file for error.out
$unf_data=read($unf) #list of trans_id
$ann_data=read($ann) #hash:{trans_id=>list_of_other_info}
$out_data=new hash
func search($ann_data,$unf_data,$out_data,$err_log)
	if $ann_data($trans_id) exist
		push $ann($trans_id) to $out_data
	else
		print error info to $err_log
search($ann_data,$unf_data,$out_data,$err_log)
